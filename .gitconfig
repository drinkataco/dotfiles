[user]
    name = J J Walwyn
    email = me@joshwalwyn.com
    username = drinkataco

[core]
    editor = vim
    excludesfile = /Users/aedd/.gitignore

[web]
    browser = google-chrome

[diff]
    tool = vimdiff

[difftool]
    prompt = false

[alias]
    a = add

    b = branch
    ba = branch -a

    c = commit
    cm = commit -m
    cam = commit -am
    cd = commit --amend
    cad = commit -a --amend

    ch = checkout
    chb = checkout -b

    d = diff
    dc = diff --cached

    f = fetch

    l = log

    m = merge
    mm = merge -X ours
    mt = merge -X theirs

    p = push
    po = push -u origin $(git branch-name)

    r = reset

    s = status

